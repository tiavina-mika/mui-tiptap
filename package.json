{
  "name": "mui-tiptap-editor",
  "description": "Tiptap WYSIWYG rich text editor using Material UI (MUI)",
  "private": false,
  "keywords": [
    "mui",
    "tiptap",
    "material-ui",
    "prosemirror",
    "rich text editor",
    "rte",
    "wysiwyg",
    "quill",
    "draftjs",
    "slate"
  ],
  "homepage": "https://github.com/tiavina-mika/mui-tiptap-editor",
  "bugs": {
    "url": "https://github.com/tiavina-mika/mui-tiptap-editor/issues"
  },
  "author": {
    "name": "Tiavina Michael Ralainirina",
    "email": "tiavinamika@gmail.com",
    "github": "https://github.com/tiavina-mika"
  },
  "version": "0.1.31",
  "license": "MIT",
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/esm/index.js"
    },
    "./icons": {
      "types": "./dist/icons/index.d.ts",
      "require": "./dist/icons/index.js",
      "import": "./dist/esm/icons/index.js"
    }
  },
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rimraf dist",
    "dev": "vite",
    "build": "yarn clean && tsc --project tsconfig.build.json && tsc --project tsconfig.build-esm.json && yarn copy-files && yarn copy-files-esm",
    "prepare": "npm run build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --fix --report-unused-disable-directives --max-warnings 0",
    "visualize": "npx vite-bundle-visualizer",
    "copy-files": "copyfiles -u 1 src/*.css dist/",
    "copy-files-esm": "copyfiles -u 1 src/*.css dist/esm"
  },
  "dependencies": {
    "vite": "5.0.8",
    "vite-tsconfig-paths": "^4.3.1"
  },
  "peerDependencies": {
    "@emotion/react": "^11.11.3",
    "@mui/material": "^5.15.2",
    "@tiptap/pm": "^2.2.5",
    "@tiptap/react": "^2.2.5",
    "react": "^16.8.0 || ^17.0.2 || ^18.2.0",
    "react-dom": "^16.8.0 || ^17.0.2 || ^18.2.0"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.11.0",
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.5",
    "@mui/material": "^5.15.2",
    "@tiptap/extension-code-block-lowlight": "^2.3.0",
    "@tiptap/extension-color": "^2.2.5",
    "@tiptap/extension-gapcursor": "2.3.0",
    "@tiptap/extension-heading": "^2.2.5",
    "@tiptap/extension-link": "^2.2.5",
    "@tiptap/extension-placeholder": "^2.2.5",
    "@tiptap/extension-table": "2.3.0",
    "@tiptap/extension-table-cell": "2.3.0",
    "@tiptap/extension-table-header": "2.3.0",
    "@tiptap/extension-table-row": "2.3.0",
    "@tiptap/extension-text": "^2.2.5",
    "@tiptap/extension-text-align": "2.3.0",
    "@tiptap/extension-text-style": "^2.2.5",
    "@tiptap/extension-typography": "^2.2.5",
    "@tiptap/extension-underline": "^2.2.5",
    "@tiptap/extension-youtube": "2.3.0",
    "@tiptap/pm": "^2.2.5",
    "@tiptap/react": "^2.2.5",
    "@tiptap/starter-kit": "^2.2.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.20.0",
    "@vitejs/plugin-react": "^4.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "^14.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-prefer-arrow-functions": "^3.2.4",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "lowlight": "3.1.0",
    "prettier": "^3.2.2",
    "prettier-plugin-organize-imports": "^3.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2"
  }
}
